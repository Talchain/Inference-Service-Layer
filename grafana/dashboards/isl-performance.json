{
  "dashboard": {
    "title": "ISL Performance & Observability",
    "tags": ["isl", "performance", "observability"],
    "timezone": "utc",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Request Overview",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total[5m]))",
            "legendFormat": "RPS"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "textMode": "value_and_name"
        }
      },
      {
        "id": 2,
        "title": "P50 Latency",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "P50 (ms)"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 300, "color": "yellow"},
              {"value": 500, "color": "red"}
            ]
          },
          "unit": "ms"
        }
      },
      {
        "id": 3,
        "title": "P95 Latency",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "P95 (ms)"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 1000, "color": "yellow"},
              {"value": 3000, "color": "red"}
            ]
          },
          "unit": "ms"
        }
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(http_errors_total[5m])) / sum(rate(http_requests_total[5m]))",
            "legendFormat": "Error %"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 0.01, "color": "yellow"},
              {"value": 0.05, "color": "red"}
            ]
          },
          "unit": "percentunit"
        }
      },
      {
        "id": 5,
        "title": "Requests/Second by Endpoint",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total[5m])) by (endpoint)",
            "legendFormat": "{{endpoint}}"
          }
        ],
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"}
        }
      },
      {
        "id": 6,
        "title": "Latency Percentiles",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "P99"
          }
        ],
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"}
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ms"
          }
        }
      },
      {
        "id": 7,
        "title": "Endpoint Latency Heatmap",
        "type": "heatmap",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "sum(rate(http_request_duration_seconds_bucket[5m])) by (le, endpoint)",
            "format": "heatmap",
            "legendFormat": "{{endpoint}}"
          }
        ],
        "options": {
          "calculate": false,
          "yAxis": {
            "unit": "ms"
          }
        }
      },
      {
        "id": 8,
        "title": "Success Rate by Endpoint",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{status=\"200\"}[5m])) by (endpoint) / sum(rate(http_requests_total[5m])) by (endpoint)",
            "legendFormat": "{{endpoint}}"
          }
        ],
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"}
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        }
      },
      {
        "id": 9,
        "title": "Error Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "sum(rate(http_errors_total[5m])) by (error_code)",
            "legendFormat": "{{error_code}}"
          }
        ]
      },
      {
        "id": 10,
        "title": "Circuit Breaker States",
        "type": "stat",
        "gridPos": {"h": 6, "w": 24, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "isl_circuit_breaker_state",
            "legendFormat": "{{name}}"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 1, "color": "yellow"},
              {"value": 2, "color": "red"}
            ]
          },
          "mappings": [
            {"type": "value", "options": {"0": {"text": "CLOSED"}}},
            {"type": "value", "options": {"1": {"text": "HALF_OPEN"}}},
            {"type": "value", "options": {"2": {"text": "OPEN"}}}
          ]
        }
      },
      {
        "id": 11,
        "title": "Fallback Trigger Rate",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 34},
        "targets": [
          {
            "expr": "sum(rate(isl_fallback_triggered_total[5m])) by (service)",
            "legendFormat": "{{service}}"
          }
        ],
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"}
        }
      },
      {
        "id": 12,
        "title": "Service Health Status",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 34},
        "targets": [
          {
            "expr": "isl_service_health_status",
            "legendFormat": "{{service}}"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "red"},
              {"value": 0.5, "color": "yellow"},
              {"value": 1, "color": "green"}
            ]
          },
          "mappings": [
            {"type": "value", "options": {"1": {"text": "HEALTHY"}}},
            {"type": "value", "options": {"0.5": {"text": "DEGRADED"}}},
            {"type": "value", "options": {"0": {"text": "FAILING"}}}
          ]
        }
      },
      {
        "id": 13,
        "title": "Cache Hit Rate",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 42},
        "targets": [
          {
            "expr": "isl_cache_hit_rate",
            "legendFormat": "Hit Rate"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "red"},
              {"value": 0.5, "color": "yellow"},
              {"value": 0.7, "color": "green"}
            ]
          },
          "unit": "percentunit"
        }
      },
      {
        "id": 14,
        "title": "Cache Size",
        "type": "stat",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 42},
        "targets": [
          {
            "expr": "isl_cache_size",
            "legendFormat": "Cache Size"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "unit": "short"
        }
      },
      {
        "id": 15,
        "title": "Cache Eviction Rate",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 42},
        "targets": [
          {
            "expr": "rate(isl_cache_evictions_total[5m])",
            "legendFormat": "Evictions/sec"
          }
        ],
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"}
        }
      },
      {
        "id": 16,
        "title": "Memory Usage",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 50},
        "targets": [
          {
            "expr": "process_resident_memory_bytes / 1024 / 1024",
            "legendFormat": "Memory (MB)"
          }
        ],
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"}
        },
        "fieldConfig": {
          "defaults": {
            "unit": "decmbytes"
          }
        }
      },
      {
        "id": 17,
        "title": "Active Requests",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 50},
        "targets": [
          {
            "expr": "active_requests",
            "legendFormat": "Active Requests"
          }
        ],
        "options": {
          "legend": {"displayMode": "list", "placement": "bottom"}
        }
      }
    ]
  },
  "overwrite": true
}
